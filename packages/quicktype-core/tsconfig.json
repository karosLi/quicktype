{
    "compilerOptions": {
        "target": "es6",
        "module": "commonjs",
        "lib": [
            "es6",
            "esnext.asynciterable"
        ],
        "allowJs": false,
        "declaration": true,
        "strict": true,
        "outDir": "dist",
        "noUnusedLocals": false,
        "noUnusedParameters": false,
        "noFallthroughCasesInSwitch": true,
        "esModuleInterop": true,
        "baseUrl": "src"
    },
    "include": [
        "src"
    ]
}